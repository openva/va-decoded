<phpunit
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://schema.phpunit.de/4.5/phpunit.xsd"
  bootstrap="bootstrap.php"
  backupGlobals="true">
  <testsuites>
    <testsuite name="Tests">
      <directory suffix="Test.php">.*</directory>
    </testsuite>
  </testsuites>
</phpunit>
